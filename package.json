  "description": "A React hook that allows you to control a CSS `scroll-snap` slider.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts",
