    "react-intersection-observer": "^9.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "tsup": "^6.7.0",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4"
  }
}
